<?xml version="1.0" ?>
<ber:model xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:se="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc" xmlns:sld="http://www.opengis.net/sld" xmlns:ber="http://www.berit.com/ber">
    <ber:project name="Fish and Ponds Information System" version="1.0">
        <ber:spatialInfo baseUnit="meters" srs="EPSG:4326">
            <ber:range>
                <ber:x min="" max=""/>
                <ber:y min="" max=""/>
            </ber:range>
            <ber:tolerance>0.01</ber:tolerance>
        </ber:spatialInfo>
    </ber:project>
    <ber:system id="SAMO">
        <ber:version>
            <ber:model>1.0</ber:model>
            <ber:minClient>1.0</ber:minClient>
            <ber:minAS>1.0</ber:minAS>
        </ber:version>
    </ber:system>
    <ber:numberGeneratorArray>
        
    </ber:numberGeneratorArray>
    <ber:containerArray>
        
        <ber:container id="ct_boPond" name="Pond Container" dbName="CT_BOPOND" versioned="true"/>
        
        <ber:container id="ct_boFish" name="Fish Container" dbName="CT_BOFISH" versioned="true"/>
        
        <ber:container id="ct_boSponsor" name="Sponsor Container" dbName="CT_BOSPONSOR" versioned="true"/>
        
        <ber:container id="ct_boExtParty" name="External Party Container" dbName="CT_BOEXTPARTY" versioned="true"/>
        
    </ber:containerArray>
    <ber:codeListArray>
        
        <ber:codeList id="cl_boPondType" name="Pond Type" dbName="CL_BOPONDTYPE" size="small">
            <ber:columnArray>
                
                <ber:column id="ca_boPondType_id" name="id" dbName="ID" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:column>
                
                <ber:column id="ca_boPondType_code" name="code" dbName="CODE" nillable="false">
                    <ber:string maxLength="20"/>
                </ber:column>
                
                <ber:column id="ca_boPondType_description" name="description" dbName="DESCRIPTION" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:column>
                
            </ber:columnArray>
        </ber:codeList>
        
        <ber:codeList id="cl_boFishSpecies" name="Fish Species" dbName="CL_BOFISHSPECIES" size="small">
            <ber:columnArray>
                
                <ber:column id="ca_boFishSpecies_id" name="id" dbName="ID" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:column>
                
                <ber:column id="ca_boFishSpecies_code" name="code" dbName="CODE" nillable="false">
                    <ber:string maxLength="20"/>
                </ber:column>
                
                <ber:column id="ca_boFishSpecies_description" name="description" dbName="DESCRIPTION" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:column>
                
            </ber:columnArray>
        </ber:codeList>
        
        <ber:codeList id="LDD_Common::taCity" name="City" dbName="CL_TACITY" size="small">
            <ber:columnArray>
                
                <ber:column id="ca_taCity_id" name="id" dbName="ID" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:column>
                
                <ber:column id="ca_taCity_code" name="code" dbName="CODE" nillable="false">
                    <ber:string maxLength="20"/>
                </ber:column>
                
                <ber:column id="ca_taCity_description" name="description" dbName="DESCRIPTION" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:column>
                
            </ber:columnArray>
        </ber:codeList>
        
        <ber:codeList id="boSecWorkPlace" name="Security Work Place" dbName="CL_SECPROVPLACED" size="small">
            <ber:columnArray>
                
                <ber:column id="ca_boSecWorkPlace_id" name="id" dbName="ID" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:column>
                
                <ber:column id="ca_boSecWorkPlace_code" name="code" dbName="CODE" nillable="false">
                    <ber:string maxLength="20"/>
                </ber:column>
                
                <ber:column id="ca_boSecWorkPlace_description" name="description" dbName="DESCRIPTION" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:column>
                
            </ber:columnArray>
        </ber:codeList>
        
    </ber:codeListArray>
    <ber:codeListBindingArray>
        
    </ber:codeListBindingArray>
    <ber:attachmentTypeArray>
        
    </ber:attachmentTypeArray>
    <ber:featureTypeArray>
        <ber:rootFeatureType id="ft_5000002" name="Root" abstract="true">
            <ber:description>ROOT_NODE</ber:description>
            <ber:featureAttributeArray>
                <ber:systemAttribute id="id" name="Feature ID"/>
                <ber:systemAttribute id="sid" name="Semantic ID"/>
                <ber:systemAttribute id="ftid" name="Feature Type"/>
                <ber:systemAttribute id="createdBy" name="Created_by"/>
                <ber:systemAttribute id="updatedBy" name="Updated_by"/>
                <ber:systemAttribute id="createDate" name="Create_date"/>
                <ber:systemAttribute id="updateDate" name="Update_date"/>
                <ber:systemAttribute id="longTransactionId" name="Long_trans_ID"/>
                <ber:systemAttribute id="symbologyTokens" name="Symbology tokens"/>
                <ber:systemAttribute id="validFrom" name="Valid From"/>
                <ber:systemAttribute id="validTo" name="Valid To"/>
                <ber:systemAttribute id="featureInfo" name="Feature Info"/>
            </ber:featureAttributeArray>
        </ber:rootFeatureType>
        
        <ber:featureType id="ft_boPond" name="Pond" parentId="" abstract="false">
            <ber:container refId="ct_boPond"/>
            <ber:featureAttributeArray>
                
                <ber:attribute id="at_boPond_code" name="code" dbName="CODE_T" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:attribute>
                
                <ber:attribute id="at_boPond_description" name="description" dbName="DESCRIPTION_T" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:attribute>
                
                <ber:attribute id="at_boPond_areaSize" name="areaSize" dbName="AREASIZE" nillable="true">
                    <ber:string maxLength="15"/>
                </ber:attribute>
                
                <ber:attribute id="at_boPond_dateOrigin" name="dateOrigin" dbName="DATEORIGIN_T" nillable="true">
                    <ber:string maxLength="10"/>
                </ber:attribute>
                
                <ber:attribute id="at_boPond_managementDesc" name="managementDesc" dbName="MANAGEMENTDESC" nillable="true">
                    <ber:string maxLength="500"/>
                </ber:attribute>
                
            </ber:featureAttributeArray>
        </ber:featureType>
        
        <ber:featureType id="ft_boFish" name="Fish" parentId="" abstract="false">
            <ber:container refId="ct_boFish"/>
            <ber:featureAttributeArray>
                
                <ber:attribute id="at_boFish_cl_fishSpecies" name="species" dbName="CL_FISHSPECIES" nillable="true">
                    <ber:string maxLength="30"/>
                </ber:attribute>
                
                <ber:attribute id="at_boFish_amount" name="amount" dbName="AMOUNT" nillable="true">
                    <ber:string maxLength="10"/>
                </ber:attribute>
                
            </ber:featureAttributeArray>
        </ber:featureType>
        
        <ber:featureType id="ft_boSponsor" name="Sponsor" parentId="" abstract="false">
            <ber:container refId="ct_boSponsor"/>
            <ber:featureAttributeArray>
                
                <ber:attribute id="at_boSponsor_code" name="code" dbName="CODE_T" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:attribute>
                
                <ber:attribute id="at_boSponsor_name" name="name" dbName="NAME_T" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:attribute>
                
            </ber:featureAttributeArray>
        </ber:featureType>
        
        <ber:featureType id="ft_boExtParty" name="External Party" parentId="" abstract="true">
            <ber:container refId="ct_boExtParty"/>
            <ber:featureAttributeArray>
                
                <ber:attribute id="at_boExtParty_code" name="code" dbName="CODE_T" nillable="false">
                    <ber:string maxLength="10"/>
                </ber:attribute>
                
                <ber:attribute id="at_boExtParty_userIdProperty" name="userIdProperty" dbName="USERIDPROPERTY" nillable="true">
                    <ber:string maxLength="50"/>
                </ber:attribute>
                
            </ber:featureAttributeArray>
        </ber:featureType>
        
        <ber:featureType id="ft_boPerson" name="Person" parentId="ft_boExtParty" abstract="false">
            <ber:container refId="ct_boExtParty"/>
            <ber:featureAttributeArray>
                
                <ber:attribute id="at_boPerson_dateOfBirth" name="dateOfBirth" dbName="DATEOFBIRTH" nillable="true">
                    <ber:string maxLength="10"/>
                </ber:attribute>
                
                <ber:attribute id="at_boPerson_firstName" name="firstName" dbName="FIRSTNAME" nillable="true">
                    <ber:string maxLength="50"/>
                </ber:attribute>
                
                <ber:attribute id="at_boPerson_surname" name="surname" dbName="SURNAME" nillable="true">
                    <ber:string maxLength="50"/>
                </ber:attribute>
                
            </ber:featureAttributeArray>
        </ber:featureType>
        
        <ber:featureType id="ft_boCompany" name="Company" parentId="ft_boExtParty" abstract="false">
            <ber:container refId="ct_boExtParty"/>
            <ber:featureAttributeArray>
                
                <ber:attribute id="at_boCompany_companyName" name="companyName" dbName="COMPANYNAME" nillable="true">
                    <ber:string maxLength="255"/>
                </ber:attribute>
                
                <ber:attribute id="at_boCompany_CIN" name="CIN" dbName="CIN" nillable="true">
                    <ber:string maxLength="20"/>
                </ber:attribute>
                
                <ber:attribute id="at_boCompany_CID" name="CID" dbName="CID" nillable="true">
                    <ber:string maxLength="20"/>
                </ber:attribute>
                
            </ber:featureAttributeArray>
        </ber:featureType>
        
    </ber:featureTypeArray>
    <ber:roleTypeArray>
        
    </ber:roleTypeArray>
    <ber:assocTypeArray>
        
        <ber:featureRefRelationAssoc id="assoc_boPond_boFish" name="Pond-Fish Association">
            <ber:masterRole refId="ft_boPond" cardinality="1"/>
            <ber:childRole refId="ft_boFish" cardinality="M"/>
        </ber:featureRefRelationAssoc>
        
        <ber:featureRefRelationAssoc id="assoc_boPond_boSponsor" name="Pond-Sponsor Association">
            <ber:masterRole refId="ft_boPond" cardinality="1"/>
            <ber:childRole refId="ft_boSponsor" cardinality="M"/>
        </ber:featureRefRelationAssoc>
        
        <ber:featureRefRelationAssoc id="assoc_boPond_boExtParty" name="Pond-External Party Association">
            <ber:masterRole refId="ft_boPond" cardinality="1"/>
            <ber:childRole refId="ft_boExtParty" cardinality="M"/>
        </ber:featureRefRelationAssoc>
        
    </ber:assocTypeArray>
</ber:model>
